{
  "compilerOptions": {
    "target": "es2019",
    "module": "CommonJS",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "allowJs": true
  }
}